ARCH=mips
VENDOR=malta
TC_CFG=$ARCH-$VENDOR-linux-gnu

PREFIX=/opt/clab/$VENDOR
CROSS_COMPILE=$PREFIX/bin/${TC_CFG}-
BUILD_TOOLCHAIN_CONFIG=build-configs/toolchain/ct-ng-1.13.0-$TC_CFG/config
BUILD_QEMU_CONFIG=build-configs/qemu/qemu-0.15.1-$ARCH
BUILD_BAREBOX_CONFIG=build-configs/barebox/barebox-2011.10.0-$VENDOR/config
BUILD_LINUX_CONFIG=build-configs/linux/linux-2.6.38.8-$VENDOR/config
BUILD_ROOTFS_CONFIG=build-configs/rootfs-default/config
BUILD_BUSYBOX_CONFIG=$BUILD_ROOTFS_CONFIG
BUILD_DROPBEAR_CONFIG=$BUILD_ROOTFS_CONFIG
BUILD_KEXEC_TOOLS_CONFIG=$BUILD_ROOTFS_CONFIG
